eval "`dircolors`"
function gbsrcipaddr() {
  _a=`echo $SSH_CONNECTION|cut -d " " -f 1`
  echo $_a
}
function gbdstipaddr() {
  _a=`echo $SSH_CONNECTION|cut -d " " -f 3`
  echo $_a
}
function sz64() {
  if [[ $1 == *.bz2 ]] ; then
    a=`base64 -w0 -- $1`
    echo " "echo \"$a\" \| base64 -d \> $1
  else
    a=`bzip2 --best -c -- $1 | base64 -w0`
    echo " "echo \"$a\" \| base64 -d \| bunzip2 \> $1
  fi
}
